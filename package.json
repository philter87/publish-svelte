{
  "name": "publish-svelte",
  "version": "0.8.0",
  "description": "A command-line tool for publishing svelte components to npm",
  "prepublishOnly": "tsc",
  "main": "dist/pelte.js",
  "scripts": {
    "test": "mocha -r ts-node/register test/**.ts",
    "tsc": "tsc"
  },
  "bin": {
    "pelte": "dist/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "svelte",
    "cli",
    "publish",
    "share",
    "npm",
    "components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/philter87/publish-svelte"
  },
  "author": "Philip Christiansen",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@tsconfig/svelte": "^1.0.0",
    "commander": "^4.0.1",
    "postcss": "^8.1.4",
    "prompts": "^2.3.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^7.0.0",
    "svelte": "^3.12.1",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.6.1",
    "lodash": "^4.17.21",
    "mocha": "^10.1.0",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "locate-character": "^2.0.5"
  }
